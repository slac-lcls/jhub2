[Unit]
Description=JupyterhubTest
After=syslog.target network.target

[Service]
User=root
SyslogIdentifier=hubtest
Environment="PYTHONPATH=/u1/jupyterhub/test/jhub2"
Environment="PATH=/cds/sw/ds/dm/conda/envs/jhub-1.4.2/bin:/sbin/:/usr/bin/:/bin"
ExecStart=/cds/sw/ds/dm/conda/envs/jhub-1.4.2/bin/jupyterhub --config jupyterhub_test_config.py
WorkingDirectory=/u1/jupyterhub/test/jhub2

[Install]
WantedBy=multi-user.target

